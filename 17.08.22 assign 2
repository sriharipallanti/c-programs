#include <stdio.h>

int main()
{
    int n1=0,n2=1,n3,i,n;
    printf("enter the number of elements");
    scanf("%d",&n);
    printf("%d%d",n1,n2);
    for(i=3;i<=n;i++)
    {
        n3=n1+n2;
        printf("%d",n3);
        n1=n2;
        n2=n3;
    }
    

    return 0;
}



output:
enter the number of elements12
01123581321345589
